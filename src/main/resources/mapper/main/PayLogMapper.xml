<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yedam.erp.mapper.main.PayLogMapper">
	<select id="payLogList" parameterType="String" resultType="PayLogVO">
		select * 
		from pay_log
		where sub_code = #{subCode}
	</select>
</mapper>