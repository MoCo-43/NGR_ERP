	<parameter name="xpCodeParam" class="java.lang.String"/>
	<query language="SQL"><![CDATA[SELECT 
    od.PRODUCT_CODE AS "productCode",
    od.PRODUCT_NAME AS "productName",
    od.QTY AS "qty",
    od.ORDER_PRICE AS "orderPrice",
    od.SUP_AMT AS "supAmt",
    od.VAT_AMT AS "vatAmt"
FROM order_req om
JOIN order_detail od ON om.ORDER_CODE = od.ORDER_CODE
JOIN customer c ON om.CUS_CODE = c.CUS_CODE
JOIN explain ex ON ex.xp_code = om.xp_code
WHERE om.xp_code =  $P{xpCodeParam} 
ORDER BY od.OD_NO]]></query>
