	<query language="SQL"><![CDATA[SELECT
    d.dept_code,
    d.dept_name,
    e.emp_id,
    e.name        AS emp_name,
    e.position    AS position,

    NVL(a.base_pay,      NVL(epf.base_pay,0))      AS base_pay,
    NVL(a.family_allow,  NVL(epf.family_allow,0))  AS family_allow,
    NVL(a.meal_allow,    NVL(epf.meal_allow,0))    AS meal_allow,
    NVL(a.annual_allow,  NVL(epf.annual_allow,0))  AS annual_allow,

    ( NVL(a.base_pay,      NVL(epf.base_pay,0))
    + NVL(a.family_allow,  NVL(epf.family_allow,0))
    + NVL(a.meal_allow,    NVL(epf.meal_allow,0))
    + NVL(a.annual_allow,  NVL(epf.annual_allow,0)) )                AS total_allow,

    NVL(ded.income_tax,0)       AS income_tax,
    NVL(ded.resident_tax,0)     AS resident_tax,
    NVL(ded.national_pension,0) AS national_pension,
    NVL(ded.health_ins,0)       AS health_ins,
    NVL(ded.employ_ins,0)       AS employ_ins,

    ( NVL(ded.income_tax,0)
    + NVL(ded.resident_tax,0)
    + NVL(ded.national_pension,0)
    + NVL(ded.health_ins,0)
    + NVL(ded.employ_ins,0) )                                       AS total_deduct,

    ( ( NVL(a.base_pay,      NVL(epf.base_pay,0))
      + NVL(a.family_allow,  NVL(epf.family_allow,0))
      + NVL(a.meal_allow,    NVL(epf.meal_allow,0))
      + NVL(a.annual_allow,  NVL(epf.annual_allow,0)) )
     - ( NVL(ded.income_tax,0)
      + NVL(ded.resident_tax,0)
      + NVL(ded.national_pension,0)
      + NVL(ded.health_ins,0)
      + NVL(ded.employ_ins,0) ) )                                   AS net_pay

FROM emp e
JOIN dept d
  ON d.dept_code    = e.dept_code
 AND d.company_code = e.company_code
LEFT JOIN emp_pay_fix epf
  ON epf.emp_id = e.emp_id

/* 해당 급여차수(payroll_no) 수당 합계 */
LEFT JOIN (
    SELECT emp_id,
           SUM(CASE WHEN allow_code='BASE'   THEN allow_amount ELSE 0 END) AS base_pay,
           SUM(CASE WHEN allow_code='FAMILY' THEN allow_amount ELSE 0 END) AS family_allow,
           SUM(CASE WHEN allow_code='MEAL'   THEN allow_amount ELSE 0 END) AS meal_allow,
           SUM(CASE WHEN allow_code='ANNUAL' THEN allow_amount ELSE 0 END) AS annual_allow
      FROM emp_allow_detail
     WHERE payroll_no = $P{PAYROLL_NO}
     GROUP BY emp_id
) a ON a.emp_id = e.emp_id

/* 해당 급여차수(payroll_no) 공제 합계 */
LEFT JOIN (
    SELECT emp_id,
           SUM(CASE WHEN deduct_code='INCOME_TAX'       THEN deduct_amount ELSE 0 END) AS income_tax,
           SUM(CASE WHEN deduct_code='RESIDENT_TAX'     THEN deduct_amount ELSE 0 END) AS resident_tax,
           SUM(CASE WHEN deduct_code='NATIONAL_PENSION' THEN deduct_amount ELSE 0 END) AS national_pension,
           SUM(CASE WHEN deduct_code='HEALTH_INS'       THEN deduct_amount ELSE 0 END) AS health_ins,
           SUM(CASE WHEN deduct_code='EMPLOY_INS'       THEN deduct_amount ELSE 0 END) AS employ_ins
      FROM emp_deduct_detail
     WHERE payroll_no = $P{PAYROLL_NO}
     GROUP BY emp_id
) ded ON ded.emp_id = e.emp_id

WHERE d.dept_name    = $P{p_dept}
  AND d.company_code = $P{P_companyCode}

ORDER BY d.dept_code, e.emp_id
]]></query>
